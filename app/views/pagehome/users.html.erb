<h1>Lista de Usuarios</h1>
<div class="span10">
<table class="table table-bordered">
  <tr>
    <th>Email</th>
    <th>Bloqueado</th>
    <th>Admin?</th>

  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.status %></td>
    <td><%= user.admin  %></td>
    <td><%= link_to "edit", edit_path(user), :class=>"btn btn-primary" %></td>
  </tr>
<% end %>
</table>
<%= link_to 'Nuevo', new_user_registration_path, :class => "btn btn-large btn-primary" %>
</div>

