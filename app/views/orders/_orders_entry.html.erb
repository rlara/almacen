<% entry_list.each do |ord| %>

<tbody>
  <tr>
    <% if ord.mode == "1" %>
        <td>Salida</td>
    <% elsif ord.mode == "3" %>
        <td>Entrada(Transferencia)</td>
    <% elsif ord.mode == "2" %>
        <td>Entrada(Proveedor)</td>
    <% elsif ord.mode == "4" %>
        <td>Cierre</td>
    <% elsif ord.mode == "5" %>
        <td>Cierre(salida)</td>
    <% end %>
    <td><%= ord.branch.name %></td>
    <td><%= link_to 'show', branch_mov_prod_path(@branch,ord), :class=>"btn btn-primary" %></td>
    <td><%= link_to 'Delete', branch_order_path(@branch,ord), confirm: 'Estas seguro?', method: :delete, :class=>"btn btn-danger"%></td>

    <td></td>
  </tr>
</tbody>


<% end  %>
