<h1>Listing moves</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Order</th>
    <th>Branch</th>
    <th>Entry</th>
    <th>Output</th>
    <th>Existence</th>
    <th>Amount d</th>
    <th>Amount h</th>
    <th>Amount s</th>
    <th>Date move</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @moves.each do |move| %>
  <tr>
    <td><%= move.product %></td>
    <td><%= move.order %></td>
    <td><%= move.branch %></td>
    <td><%= move.entry %></td>
    <td><%= move.output %></td>
    <td><%= move.existence %></td>
    <td><%= move.amount_d %></td>
    <td><%= move.amount_h %></td>
    <td><%= move.amount_s %></td>
    <td><%= move.date_move %></td>
    <td><%= link_to 'Show', move %></td>
    <td><%= link_to 'Edit', edit_move_path(move) %></td>
    <td><%= link_to 'Destroy', move, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Move', new_move_path %>
