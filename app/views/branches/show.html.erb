
  <p id="notice"><%= notice %></p>

<p style="font-size:15px">
  <b>Nombre:</b>
  <%= @branch.name %>
</p>

<p style="font-size:15px">
  <b>Ubicación:</b>
  <%= @branch.location %>
  <%= link_to 'Edit', edit_branch_path(@branch), :class=>"btn" %>

