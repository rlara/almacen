        <%#= render :partial => "orders/orders_partial", :locals => {:list_ord => @orders}%>

		<h3>Entradas</h3>
		  <%= render :partial => "orders/orders_entry", :locals => {:entry_list => @order_list_entry}%>
	</div>

  <div class="span3" style="float:right">

    <h2>Movimiento Pendientes</h2>
